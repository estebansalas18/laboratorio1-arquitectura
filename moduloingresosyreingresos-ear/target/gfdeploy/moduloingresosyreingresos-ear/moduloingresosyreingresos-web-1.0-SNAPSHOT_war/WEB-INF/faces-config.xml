<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>carreraMBean</managed-bean-name>
        <managed-bean-class>com.udea.controller.CarreraMBean</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>estudianteMBean</managed-bean-name>
        <managed-bean-class>com.udea.controller.EstudianteMBean</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/index.html</from-view-id>
        <navigation-case>
            <from-outcome>Saved</from-outcome>
            <to-view-id>/CarreraForm.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Saved</from-outcome>
            <to-view-id>/EstudianteForm.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/CarreraForm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/index.html</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/EstudianteForm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/index.html</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
